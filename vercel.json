{
    "rewrites": [
        {
            "source": "/login",
            "destination": "/login.html"
        },
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/app/chat/:path*",
            "destination": "/app/index.html"
        },
        {
            "source": "/app/newchat",
            "destination": "/app/index.html"
        },
        {
            "source": "/app",
            "destination": "/app/index.html"
        },
        {
            "source": "/privacy",
            "destination": "/privacy.html"
        },
        {
            "source": "/terms",
            "destination": "/terms.html"
        },
        {
            "source": "/dpa",
            "destination": "/dpa.html"
        },
        {
            "source": "/security",
            "destination": "/security.html"
        },
        {
            "source": "/manifest",
            "destination": "/manifest.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}
