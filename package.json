{
    "name": "veridat-bokforing",
    "version": "1.0.0",
    "description": "Veridat - AI Bokföringsexpert för svenska småföretagare",
    "type": "module",
    "scripts": {
        "dev": "vite --config apps/web/vite.config.ts",
        "build": "tsc -p apps/web/tsconfig.json && vite build --config apps/web/vite.config.ts",
        "build:analyze": "VITE_BUNDLE_ANALYZE=true npm run build",
        "preview": "vite preview --config apps/web/vite.config.ts",
        "lint": "eslint apps/web/src",
        "lint:fix": "eslint apps/web/src --fix",
        "test": "vitest run --config apps/web/vitest.config.ts",
        "test:watch": "vitest --config apps/web/vitest.config.ts",
        "test:e2e": "node ./node_modules/@playwright/test/cli.js test",
        "test:agent:core": "bash ./scripts/run-core-agents.sh",
        "test:agent:core:soft": "bash ./scripts/run-core-agents-soft.sh",
        "test:agent:dashboard": "bash ./scripts/run-dashboard-agent.sh",
        "test:agent:bank": "bash ./scripts/run-bank-reconciliation-agent.sh",
        "test:agent:finance": "bash ./scripts/run-finance-agents.sh",
        "test:agent:nightly": "bash ./scripts/run-nightly-agents.sh",
        "perf:benchmark": "node scripts/benchmark-gemini-chat.mjs",
        "check:edge": "deno check supabase/functions/*/index.ts",
        "ops:smoke": "./scripts/ops-smoke.sh",
        "supabase:setup": "./scripts/setup-local-supabase.sh",
        "supabase:start": "supabase start",
        "supabase:serve": "./scripts/start-local-functions.sh",
        "supabase:serve:stop": "./scripts/stop-local-functions.sh",
        "supabase:stop": "./scripts/stop-local-functions.sh && supabase stop"
    },
    "keywords": [
        "bokföring",
        "accounting",
        "swedish",
        "ai",
        "gemini"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@playwright/mcp": "^0.0.63",
        "@playwright/test": "^1.50.0",
        "@types/node": "^20.10.0",
        "@vitest/ui": "^4.0.16",
        "eslint": "^9.39.2",
        "jsdom": "^27.4.0",
        "rollup-plugin-visualizer": "^6.0.5",
        "supabase": "^2.67.1",
        "typescript": "^5.3.0",
        "typescript-eslint": "^8.52.0",
        "vite": "^7.3.1",
        "vitest": "^4.0.16"
    },
    "dependencies": {
        "@heroui/react": "^2.8.7",
        "@preact/preset-vite": "^2.10.2",
        "@sentry/browser": "^10.38.0",
        "@supabase/supabase-js": "^2.39.0",
        "@types/dompurify": "^3.0.5",
        "clsx": "^2.1.1",
        "dompurify": "^3.3.1",
        "framer-motion": "^12.29.0",
        "pdfjs-dist": "^4.10.38",
        "preact": "^10.28.3",
        "tailwind-merge": "^3.4.0",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
    }
}
