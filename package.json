{
  "name": "veridat-bokforing",
  "version": "1.0.0",
  "description": "Veridat - AI Bokföringsexpert för svenska småföretagare",
  "type": "module",
  "scripts": {
    "dev": "vite --config apps/web/vite.config.ts",
    "build": "tsc -p apps/web/tsconfig.json && vite build --config apps/web/vite.config.ts",
    "preview": "vite preview --config apps/web/vite.config.ts",
    "lint": "eslint apps/web/src",
    "lint:fix": "eslint apps/web/src --fix",
    "test": "vitest run --config apps/web/vitest.config.ts",
    "test:watch": "vitest --config apps/web/vitest.config.ts",
    "test:e2e": "playwright test",
    "check:edge": "deno check supabase/functions/*/index.ts",
    "ops:smoke": "./scripts/ops-smoke.sh",
    "supabase:setup": "./scripts/setup-local-supabase.sh",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop"
  },
  "keywords": [
    "bokföring",
    "accounting",
    "swedish",
    "ai",
    "gemini"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.50.0",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "supabase": "^2.67.1",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.52.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@heroui/react": "^2.8.7",
    "@preact/preset-vite": "^2.10.2",
    "@supabase/supabase-js": "^2.39.0",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.29.0",
    "pdfjs-dist": "^4.10.38",
    "preact": "^10.27.2",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  }
}
